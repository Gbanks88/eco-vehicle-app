{
    "themes": {
        "eco_green": {
            "name": "Eco Green",
            "description": "Nature-inspired green theme",
            "colors": {
                "primary": {
                    "50": "#f0fdf4",
                    "100": "#dcfce7",
                    "200": "#bbf7d0",
                    "300": "#86efac",
                    "400": "#4ade80",
                    "500": "#22c55e",
                    "600": "#16a34a",
                    "700": "#15803d",
                    "800": "#166534",
                    "900": "#14532d"
                },
                "accent": {
                    "blue": "#3b82f6",
                    "purple": "#8b5cf6",
                    "yellow": "#eab308",
                    "red": "#ef4444"
                },
                "neutral": {
                    "50": "#fafafa",
                    "100": "#f4f4f5",
                    "200": "#e4e4e7",
                    "300": "#d4d4d8",
                    "400": "#a1a1aa",
                    "500": "#71717a",
                    "600": "#52525b",
                    "700": "#3f3f46",
                    "800": "#27272a",
                    "900": "#18181b"
                }
            },
            "gradients": {
                "primary": "linear-gradient(135deg, var(--color-primary-400) 0%, var(--color-primary-600) 100%)",
                "secondary": "linear-gradient(135deg, var(--color-accent-blue) 0%, var(--color-accent-purple) 100%)",
                "warning": "linear-gradient(135deg, var(--color-accent-yellow) 0%, var(--color-accent-red) 100%)"
            }
        },
        "ocean_blue": {
            "name": "Ocean Blue",
            "description": "Calming ocean-inspired theme",
            "colors": {
                "primary": {
                    "50": "#eff6ff",
                    "100": "#dbeafe",
                    "200": "#bfdbfe",
                    "300": "#93c5fd",
                    "400": "#60a5fa",
                    "500": "#3b82f6",
                    "600": "#2563eb",
                    "700": "#1d4ed8",
                    "800": "#1e40af",
                    "900": "#1e3a8a"
                },
                "accent": {
                    "teal": "#14b8a6",
                    "indigo": "#6366f1",
                    "orange": "#f97316",
                    "red": "#ef4444"
                }
            },
            "gradients": {
                "primary": "linear-gradient(135deg, var(--color-primary-400) 0%, var(--color-primary-600) 100%)",
                "secondary": "linear-gradient(135deg, var(--color-accent-teal) 0%, var(--color-accent-indigo) 100%)",
                "warning": "linear-gradient(135deg, var(--color-accent-orange) 0%, var(--color-accent-red) 100%)"
            }
        }
    },
    "typography": {
        "fonts": {
            "primary": {
                "family": "Inter, system-ui, sans-serif",
                "weights": {
                    "light": 300,
                    "regular": 400,
                    "medium": 500,
                    "semibold": 600,
                    "bold": 700
                }
            },
            "display": {
                "family": "Poppins, sans-serif",
                "weights": {
                    "regular": 400,
                    "medium": 500,
                    "semibold": 600,
                    "bold": 700
                }
            },
            "mono": {
                "family": "JetBrains Mono, monospace",
                "weights": {
                    "regular": 400,
                    "medium": 500
                }
            }
        },
        "sizes": {
            "xs": "0.75rem",
            "sm": "0.875rem",
            "base": "1rem",
            "lg": "1.125rem",
            "xl": "1.25rem",
            "2xl": "1.5rem",
            "3xl": "1.875rem",
            "4xl": "2.25rem",
            "5xl": "3rem"
        },
        "lineHeights": {
            "none": "1",
            "tight": "1.25",
            "snug": "1.375",
            "normal": "1.5",
            "relaxed": "1.625",
            "loose": "2"
        }
    },
    "spacing": {
        "0": "0",
        "1": "0.25rem",
        "2": "0.5rem",
        "3": "0.75rem",
        "4": "1rem",
        "5": "1.25rem",
        "6": "1.5rem",
        "8": "2rem",
        "10": "2.5rem",
        "12": "3rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem",
        "32": "8rem"
    },
    "shadows": {
        "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
        "base": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
        "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
        "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
        "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
        "inner": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)"
    },
    "animations": {
        "transitions": {
            "base": "all 0.3s ease",
            "fast": "all 0.15s ease",
            "slow": "all 0.5s ease"
        },
        "keyframes": {
            "fadeIn": {
                "from": {
                    "opacity": "0"
                },
                "to": {
                    "opacity": "1"
                }
            },
            "slideIn": {
                "from": {
                    "transform": "translateY(20px)",
                    "opacity": "0"
                },
                "to": {
                    "transform": "translateY(0)",
                    "opacity": "1"
                }
            },
            "pulse": {
                "0%, 100%": {
                    "opacity": "1"
                },
                "50%": {
                    "opacity": "0.5"
                }
            }
        }
    },
    "charts": {
        "colors": {
            "sequential": [
                "#3b82f6",
                "#60a5fa",
                "#93c5fd",
                "#bfdbfe",
                "#dbeafe"
            ],
            "diverging": [
                "#ef4444",
                "#f97316",
                "#eab308",
                "#22c55e",
                "#3b82f6"
            ],
            "qualitative": [
                "#3b82f6",
                "#22c55e",
                "#eab308",
                "#ef4444",
                "#8b5cf6",
                "#14b8a6"
            ]
        },
        "fonts": {
            "labels": "var(--font-primary)",
            "values": "var(--font-mono)"
        },
        "gridLines": {
            "color": "var(--color-neutral-200)",
            "width": "1px"
        },
        "tooltip": {
            "background": "var(--color-neutral-800)",
            "text": "var(--color-neutral-100)",
            "shadow": "var(--shadow-lg)"
        }
    }
}
