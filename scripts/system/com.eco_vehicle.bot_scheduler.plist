<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.eco_vehicle.bot_scheduler</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Volumes/Learn_Space/eco_vehicle_project/venv/bin/python</string>
        <string>/Volumes/Learn_Space/eco_vehicle_project/scripts/python/scheduler/bot_scheduler.py</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Volumes/Learn_Space/eco_vehicle_project</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PYTHONPATH</key>
        <string>/Volumes/Learn_Space/eco_vehicle_project</string>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/Volumes/Learn_Space/eco_vehicle_project/logs/scheduler_error.log</string>
    <key>StandardOutPath</key>
    <string>/Volumes/Learn_Space/eco_vehicle_project/logs/scheduler_output.log</string>
    <key>ThrottleInterval</key>
    <integer>30</integer>
</dict>
</plist>
